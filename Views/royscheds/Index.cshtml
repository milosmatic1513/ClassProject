@model IEnumerable<ClassProject.Models.roysched>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create", null, new { @class = "btn btn-primary" })
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.title.title1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.lorange)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.hirange)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.royalty)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.title.title1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.lorange)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.hirange)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.royalty)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { title_id = item.title_id, lorange = item.lorange, hirange = item.hirange }, new { @class = "btn btn-warning" })
                @Html.ActionLink("Details", "Details", new { title_id = item.title_id, lorange = item.lorange, hirange = item.hirange }, new { @class = "btn btn-info" })
                @Html.ActionLink("Delete", "Delete", new { title_id = item.title_id, lorange = item.lorange, hirange = item.hirange }, new { @class = "btn btn-danger" })
            </td>
        </tr>
    }

</table>
